<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="520.0" styleClass="light-grey-background" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase3.client.view.explorer.search.ProfileController">
   <children>
      <Circle fx:id="profilePic" fill="DODGERBLUE" layoutX="261.0" layoutY="60.0" radius="60.0" stroke="BLACK" strokeType="INSIDE" />
      <Label fx:id="name" alignment="CENTER" layoutX="179.0" layoutY="134.0" prefHeight="38.0" prefWidth="169.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="149.0" layoutY="255.0" text="Username:" />
      <Label fx:id="lastSeen" layoutX="263.0" layoutY="199.0" prefHeight="24.0" prefWidth="230.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="username" layoutX="260.0" layoutY="252.0" prefHeight="24.0" prefWidth="230.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="148.0" layoutY="202.0" text="Last Seen:" />
      <Label layoutX="142.0" layoutY="313.0" prefHeight="0.0" prefWidth="70.0" text="Follow State:" />
      <Label fx:id="followState" layoutX="260.0" layoutY="310.0" prefHeight="24.0" prefWidth="230.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="message" layoutX="53.0" layoutY="22.0" mnemonicParsing="false" onAction="#message" prefHeight="38.0" prefWidth="82.0" styleClass="silver-background" text="Message" />
      <Button fx:id="follow" layoutX="53.0" layoutY="71.0" mnemonicParsing="false" onAction="#follow" prefHeight="38.0" prefWidth="82.0" styleClass="silver-background" text="Follow" />
      <Button fx:id="report" layoutX="398.0" layoutY="71.0" mnemonicParsing="false" onAction="#report" prefHeight="38.0" prefWidth="82.0" styleClass="silver-background" text="Report" />
      <Button fx:id="block" layoutX="398.0" layoutY="22.0" mnemonicParsing="false" onAction="#block" prefHeight="38.0" prefWidth="82.0" styleClass="silver-background" text="Block" />
      <Label fx:id="alert" alignment="CENTER" layoutX="46.0" layoutY="411.0" prefHeight="75.0" prefWidth="429.0" textFill="#ed0a0a">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="149.0" layoutY="363.0" prefHeight="0.0" prefWidth="70.0" text="Page State:" />
      <Label fx:id="pageState" layoutX="260.0" layoutY="360.0" prefHeight="24.0" prefWidth="230.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
